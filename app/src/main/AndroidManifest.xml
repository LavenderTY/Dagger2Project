        <activity android:name=".activity.BalanceActivity" />
        <activity android:name=".activity.terminal.TerminalManagementActivity" />
        <activity android:name=".activity.CardApplicationActivity" />
        <activity android:name=".activity.CardVouchersActivity" />
        <activity android:name=".activity.PreferentialPurchaseActivity" />
